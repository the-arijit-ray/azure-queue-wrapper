{
  "name": "azure-queue-wrapper",
  "type": "commonjs",
  "version": "2.0.8",
  "description": "A wrapper package for making use of Azure Storage Queues easy in nodejs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "azure-queue",
    "azure-storage-queue",
    "azure-queue-polling",
    "cron-azure-queue-polling",
    "azure-queue-scheduling",
    "azure-message-queue",
    "wrapper",
    "utility",
    "decorator"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "lint:format": "eslint --ext .ts,.tsx,.js,.jsx --fix . && prettier -w .",
    "pack": "npm pack"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Arijit Ray",
    "email": "arijitray.ray@gmail.com",
    "url": "https://github.com/the-arijit-ray"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/storage-queue": "^12.16.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "typescript": "^4.7.4",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.7.1",
    "ts-node": "10.7.0"
  }
}
